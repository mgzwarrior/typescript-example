{
  "name": "typescript-example",
  "version": "1.0.0",
  "description": "A sample Node project using Typescript and the Express framework",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "lint": "eslint --ext .ts .",
    "lint:ejs": "ejslint src/views",
    "tsc": "tsc",
    "parcel": "parcel build src/public/js/main.ts",
    "build": "npm-run-all clean lint lint:ejs tsc copy-assets parcel",
    "initdb": "ts-node tools/initdb",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.js"
  },
  "default": "dist/public/js/index.js",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/fs-extra": "^9.0.13",
    "@types/materialize-css": "^1.0.11",
    "@types/node": "^16.11.6",
    "@types/pg": "^8.6.1",
    "@types/shelljs": "^0.8.9",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "ejs-lint": "^1.2.1",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "fs-extra": "^10.0.0",
    "nodemon": "^2.0.14",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4",
    "winston": "^3.3.3"
  },
  "dependencies": {
    "@okta/oidc-middleware": "^4.3.0",
    "axios": "^0.24.0",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "materialize-css": "^1.0.0",
    "pg": "^8.7.1",
    "pg-promise": "^10.11.1",
    "vue": "^2.6.14"
  }
}
